{
	"a.foo.example.com": {
		"alias": [ "a.foo", "a" ],
		"repositories": {
			"chrome": {
				"src": "/usr/local/src/chrome/src",
				"platforms": {
					"linux": {
						"endpoints": {"b": "tcp,127.0.0.1:9761", "c": "tcp,127.0.0.1:1245"},
						"out": "out/linux-gn",
						"mb_config": "debug_bot"
					},
					"android": {
						"endpoints": {"b": "tcp,127.0.0.1:9762", "c": "tcp,127.0.0.1:1246"},
						"out": "out/android-gn",
						"mb_config": "android_debug_bot"
					}
				}
			}
		},
		"goma_path": "/home/user/goma"
	},

	"b.foo.example.com": {
		"alias": [ "mac", "b" ],
		"repositories": {
			"chrome": {
				"src": "/src/chrome/src",
				"platforms": {
					"mac": {
						"endpoints": {"a": "tcp,127.0.0.1:9761", "c": "tcp,127.0.0.1:1245"},
						"out": "out/mac-gn",
						"mb_config": "debug_bot"
					}
				},
				"git_remote": "a",
				"git_hostname": "a"
			}
		},
		"goma_path": "/Users/user/goma"
	},

	"c.foo.example.com": {
		"alias": [ "c" ],
		"repositories": {
			"chrome": {
				"src": "C:\\src\\chrome\\src",
				"platforms": {
					"win": {
						"out": "out/win-gn",
						"mb_config": "debug_bot"
					}
				},
				"git_remote": "a",
				"git_hostname": "a"
			}
		},
		"goma_path": "C:\\Users\\user\\goma",
		"remotes": {
			"b": { "ssh_command": ["ssh", "foo", "bar"] },
			"a": { "ssh_command": ["ssh", "-Dfoo"] }
		}
	}
}
